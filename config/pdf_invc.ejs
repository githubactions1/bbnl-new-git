		height: auto;
		outline: 1cm #ffffff solid;
	}

	@page {
		size: A4;
		margin: 0;
	}

	@media print {
		.page {
			margin: 0;
			border: initial;
			border-radius: initial;
			width: initial;
			min-height: initial;
			box-shadow: initial;
			background: initial;

		}
	}

	.mainbody {
		height: 200mm;
		width: 15.cm;
	}

	label {
		font-weight: bold;
		font-size: 13px;
	}

	.intext {
		padding-left: 5px;
		border-left: solid;
		border-left-width: 1px;
	}

	.showtext {
		padding-right: 5px;
		text-align: right;
		border-left: solid;
		border-left-width: 1px;
	}

	.midtext {
		padding: 5px;
		text-align: center;
	}

	.header {
		height: 90px;
		padding: 0px;
	}

	table {
		border-collapse: collapse;
		font-size: 13px;
	}

	tr {
		border-bottom: 1px solid #000;
	}

	tr:last-child {
		border-bottom: none;
	}

	html {
		zoom: 0.75;
	}

	td {
		padding-left: 5px;
		height: 2em;

	}
</style>

